{
  "expo": {
    "name": "weather_App",
    "description": "weather_App is a React Native app, built with Expo, compatible for both Android and iOS mobile devices.",
    "slug": "weather_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./components/assets/icon.png",
    "splash": {
      "image": "./components/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./components/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./components/assets/favicon.png"
    }
  }
}
